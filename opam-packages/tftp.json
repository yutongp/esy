{
  "versions": {
    "0.1.4": {
      "version": "0.1.4", 
      "opam": {
        "url": "https://github.com/mor1/ocaml-tftp/archive/0.1.4.tar.gz", 
        "checksum": "eb6ad64af44fb43cc26fb3bb753a8bc4", 
        "files": []
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "./configure --prefix=$cur__install --${alcotest_enable:-disable}-tests", 
          "make build", 
          "cp ./tftp.opam/install ./tftp.install", 
          "make install", 
          "make tftpd FS=direct NET=socket MIRFLAGS=--no-depext --no-opam", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "tftp_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "tftp_enable": {
            "scope": "global", 
            "val": "enable"
          }, 
          "tftp_version": {
            "scope": "global", 
            "val": "0.1.4"
          }
        }
      }, 
      "dependencies": {
        "@opam/ocamlfind": "*", 
        "@opam/mirage-console": "*", 
        "@opam/lwt": ">= 2.4.7", 
        "@opam/cstruct": ">= 1.0.1 < 2.0.0", 
        "@opam/camlp4": "*", 
        "@opam/mirage": ">= 2.5.0 < 3.0.0", 
        "substs": "esy-ocaml/substs", 
        "@opam/alcotest": "*", 
        "@opam/mirage-fs-unix": "*", 
        "@opam/io-page": "*", 
        "opam-installer-bin": "esy-ocaml/opam-installer-bin", 
        "@opam/tcpip": "*"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.01.0"
      }, 
      "name": "@opam/tftp"
    }
  }
}