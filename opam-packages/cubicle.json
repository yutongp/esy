    "1.0.0": {
      "version": "1.0.0", 
      "opam": {
        "url": "http://cubicle.lri.fr/cubicle-1.0.tar.gz", 
        "checksum": "24b163eb77e6832747dccd6cc8a5d57c", 
        "files": [
          {
            "content": "bin: [\"cubicle.opt\" {\"cubicle\"}]\n", 
            "name": "cubicle.install"
          }
        ]
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "./configure --prefix $cur__install", 
          "make", 
          "make install MANDIR=$cur__man", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "cubicle_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "cubicle_version": {
            "scope": "global", 
            "val": "1.0.0"
          }, 
          "cubicle_enable": {
            "scope": "global", 
            "val": "enable"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml/substs", 
        "@opam/ocamlfind": "*", 
        "@opam/functory": "*", 
        "opam-installer-bin": "esy-ocaml/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.00.0 < 4.03.0"
      }, 
      "name": "@opam/cubicle"
    }, 
