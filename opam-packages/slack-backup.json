{
  "versions": {
    "0.1.0": {
      "version": "0.1.0", 
      "opam": {
        "url": "https://github.com/Khady/slack-backup/archive/0.1.zip", 
        "checksum": "021171eb1436d2c24a007c46f8ba2f0d", 
        "files": [
          {
            "content": "open Printf\n\nlet () =\n  let dir = Sys.argv.(1) in\n  (try Sys.chdir dir\n   with _ -> eprintf \"Cannot change directory to %s\\n%!\" dir);\n  exit (Sys.command \"ocaml setup.ml -uninstall\")\n", 
            "name": "_oasis_remove_.ml"
          }, 
          {
            "content": "etc: [\n  \"setup.ml\"\n  \"setup.data\"\n  \"setup.log\"\n  \"_oasis_remove_.ml\"\n]\n", 
            "name": "slack-backup.install"
          }
        ]
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "ocaml setup.ml -configure --prefix $cur__install", 
          "ocaml setup.ml -build", 
          "ocaml setup.ml -install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "slack_backup_version": {
            "scope": "global", 
            "val": "0.1.0"
          }, 
          "slack_backup_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "slack_backup_enable": {
            "scope": "global", 
            "val": "enable"
          }
        }
      }, 
      "dependencies": {
        "@opam/cmdliner": "*", 
        "@opam/result": "*", 
        "@opam/ocamlfind": "*", 
        "substs": "esy-ocaml/substs", 
        "@opam/slacko": "*", 
        "opam-installer-bin": "esy-ocaml/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.02.1"
      }, 
      "name": "@opam/slack-backup"
    }
  }
}