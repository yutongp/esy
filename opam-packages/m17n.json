        "exportedEnv": {
          "m17n_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "m17n_enable": {
            "scope": "global", 
            "val": "enable"
          }, 
          "m17n_version": {
            "scope": "global", 
            "val": "1.0.0"
          }
        }
      }, 
      "dependencies": {
        "@opam/ocamlfind": "*", 
        "@opam/utop": "*", 
        "@opam/ppx_deriving": "*", 
        "@opam/sedlex": "*", 
        "@opam/uucp": ">= 1.1.0 < 2.0.0", 
        "substs": "esy-ocaml/substs", 
        "@opam/uunf": ">= 1.0.0 < 2.0.0", 
        "@opam/gen": ">= 0.2.3 < 0.5.0", 
        "@opam/ounit": "*", 
        "@opam/uutf": ">= 0.9.4 < 1.0.0", 
        "opam-installer-bin": "esy-ocaml/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": "= 4.03.0"
      }, 
      "name": "@opam/m17n"
    }
  }
