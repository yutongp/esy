            "content": "#!/bin/sh\n\nconfig=\"llvm-config-3.4\"\n\nif ! which $config ; then\n    [ \"os-$1\" == \"os-darwin\" ] && config=\"llvm-config-mp-3.4\"\nfi\n\ncat > conf-llvm.config <<EOF\nconfig: \"$config\"\nversion: \"`$config --version`\"\nEOF\n", 
            "name": "configure"
          }
        ]
