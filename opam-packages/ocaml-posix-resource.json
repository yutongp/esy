          "ocaml setup.ml -install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "ocaml_posix_resource_version": {
            "scope": "global", 
            "val": "0.0.1"
          }, 
          "ocaml_posix_resource_enable": {
            "scope": "global", 
            "val": "enable"
          }, 
          "ocaml_posix_resource_installed": {
            "scope": "global", 
            "val": "true"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml/substs", 
        "@opam/ocamlfind": "*", 
        "@opam/oasis": ">= 0.3.0 < 0.4.7", 
        "@opam/ounit": "*", 
        "opam-installer-bin": "esy-ocaml/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/ocaml-posix-resource"
    }
  }
