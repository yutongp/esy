        "files": [
          {
            "content": "diff -ru pgocaml.1.6/Makefile pgocaml.1.6/Makefile\n--- pgocaml.1.6/Makefile\t2012-07-04 19:53:31.000000000 +0200\n+++ pgocaml.1.6/Makefile\t2012-10-05 18:14:39.889285053 +0200\n@@ -166,7 +166,7 @@\n #\n \n findlib_install:\n-\tocamlfind install $(PACKAGE) META pgocaml.a pgocaml.cma pgocaml.cmxa pGOCaml_generic.cm[ix] pGOCaml.cm[ix] pa_pgsql.cmo pGOCaml_generic.mli pGOCaml.mli\n+\tocamlfind install $(PACKAGE) META pgocaml.cmxs pgocaml.a pgocaml.cma pgocaml.cmxa pGOCaml_generic.cm[ix] pGOCaml.cm[ix] pa_pgsql.cmo pGOCaml_generic.mli pGOCaml.mli\n \n reinstall:\n \tocamlfind remove $(PACKAGE)\n", 
            "name": "opam.patch"
          }
        ]
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
