{
  "versions": {
    "0.3.0": {
      "version": "0.3.0", 
      "opam": {
        "url": "http://cubicle.lri.fr/alt-ergo-zero/aez-0.3.tar.gz", 
        "checksum": "42260d8e110defa9c8c5b6cc1eafa29f", 
        "files": [
          {
            "content": "name=\"aez\"\nversion=\"0.3\"\ndescription=\"Alt-Ergo Zero is an OCaml library for an SMT solver.\"\nrequires=\"num unix\"\narchive(byte) = \"aez.cma\"\narchive(native) = \"aez.cmxa\"\n", 
            "name": "META"
          }
        ]
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "./configure", 
          "make", 
          "ocamlfind install aez aez.a aez.cmi aez.cma aez.cmxa META", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "aez_enable": {
            "scope": "global", 
            "val": "enable"
          }, 
          "aez_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "aez_version": {
            "scope": "global", 
            "val": "0.3.0"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml/substs", 
        "@opam/ocamlfind": "*", 
        "opam-installer-bin": "esy-ocaml/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 3.12.0"
      }, 
      "name": "@opam/aez"
    }
  }
}