        "checksum": "c98cb87d60ea1e1415dc05b4bf7b84f0", 
        "files": []
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "make", 
          "make install BINDIR=$cur__bin", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "camldm_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "camldm_version": {
            "scope": "global", 
            "val": "0.1.0"
          }, 
          "camldm_enable": {
            "scope": "global", 
            "val": "enable"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml/substs", 
        "@opam/ocamlfind": "*", 
        "@opam/rpc": ">= 1.5.0", 
        "opam-installer-bin": "esy-ocaml/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/camldm"
    }
  }
}