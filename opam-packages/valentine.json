{
  "versions": {
    "1.0.0": {
      "version": "1.0.0", 
      "opam": {
        "url": "https://github.com/fxfactorial/valentine/archive/v1.0.0.tar.gz", 
        "checksum": "75c51f1b07f0f56fb647a09c26ea4610", 
        "files": [
          {
            "content": "open Printf\n\nlet () =\n  let dir = Sys.argv.(1) in\n  (try Sys.chdir dir\n   with _ -> eprintf \"Cannot change directory to %s\\n%!\" dir);\n  exit (Sys.command \"ocaml setup.ml -uninstall\")\n", 
            "name": "_oasis_remove_.ml"
          }, 
          {
            "content": "etc: [\n  \"setup.ml\"\n  \"setup.data\"\n  \"setup.log\"\n  \"_oasis_remove_.ml\"\n]\n", 
            "name": "valentine.install"
          }
        ]
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "oasis setup", 
          "ocaml setup.ml -configure --prefix $cur__install", 
          "ocaml setup.ml -build", 
          "ocaml setup.ml -install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "valentine_enable": {
            "scope": "global", 
            "val": "enable"
          }, 
          "valentine_version": {
            "scope": "global", 
            "val": "1.0.0"
          }, 
          "valentine_installed": {
            "scope": "global", 
            "val": "true"
          }
        }
      }, 
      "dependencies": {
        "@opam/cmdliner": "*", 
        "@opam/oasis": ">= 0.4.0", 
        "@opam/ocamlfind": "*", 
        "substs": "esy-ocaml/substs", 
        "@opam/markup": "*", 
        "opam-installer-bin": "esy-ocaml/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.02.0"
      }, 
      "name": "@opam/valentine"
    }
  }
}