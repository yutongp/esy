{
  "versions": {
    "1.0.0": {
      "version": "1.0.0", 
      "opam": {
        "url": "https://github.com/fxfactorial/ocaml-corecount/archive/v1.0.0.tar.gz", 
        "checksum": "e73e83bfaa0fc9f2434ebbd8cad590b6", 
        "files": []
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "oasis setup", 
          "ocaml setup.ml -configure --prefix $cur__install", 
          "ocaml setup.ml -build", 
          "ocaml setup.ml -install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "corecount_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "corecount_enable": {
            "scope": "global", 
            "val": "enable"
          }, 
          "corecount_version": {
            "scope": "global", 
            "val": "1.0.0"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml/substs", 
        "@opam/ocamlfind": "*", 
        "@opam/oasis": ">= 0.4.0", 
        "opam-installer-bin": "esy-ocaml/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.03.0"
      }, 
      "name": "@opam/corecount"
    }
  }
}