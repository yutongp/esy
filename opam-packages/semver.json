{
  "versions": {
    "0.1.0": {
      "version": "0.1.0", 
      "opam": {
        "url": "https://github.com/rgrinberg/ocaml-semver/archive/v0.1.0.tar.gz", 
        "checksum": "ce6614ba2f91754028b29a12989f9da6", 
        "files": []
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "ocaml setup.ml -configure", 
          "ocaml setup.ml -build", 
          "ocaml setup.ml -install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "semver_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "semver_enable": {
            "scope": "global", 
            "val": "enable"
          }, 
          "semver_version": {
            "scope": "global", 
            "val": "0.1.0"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml/substs", 
        "@opam/ocamlfind": "*", 
        "@opam/ounit": "*", 
        "opam-installer-bin": "esy-ocaml/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.02.0"
      }, 
      "name": "@opam/semver"
    }
  }
}