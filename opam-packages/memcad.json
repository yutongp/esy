{
  "versions": {
    "1.0.0": {
      "version": "1.0.0", 
      "opam": {
        "url": "https://github.com/Antique-team/memcad/archive/v1.0.0.tar.gz", 
        "checksum": "7951413755c9cce76409ac792c960662", 
        "files": []
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "make", 
          "mv batch memcad_batch", 
          "mv analyze memcad_analyze", 
          "cp memcad_batch $cur__bin", 
          "cp memcad_analyze $cur__bin", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "memcad_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "memcad_enable": {
            "scope": "global", 
            "val": "enable"
          }, 
          "memcad_version": {
            "scope": "global", 
            "val": "1.0.0"
          }
        }
      }, 
      "dependencies": {
        "@opam/ounit": "*", 
        "@opam/parmap": "*", 
        "@opam/clangml-transforms": "*", 
        "@opam/conf-graphviz": "*", 
        "@opam/qtest": "*", 
        "@opam/apron": "*", 
        "substs": "esy-ocaml/substs", 
        "@opam/bdd": "*", 
        "@opam/setr": "*", 
        "@opam/obuild": "*", 
        "opam-installer-bin": "esy-ocaml/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.02.0"
      }, 
      "name": "@opam/memcad"
    }
  }
}