{
  "versions": {
    "0.9.0": {
      "version": "0.9.0", 
      "opam": {
        "url": "https://github.com/talex5/mini-os/archive/v0.9.tar.gz", 
        "checksum": "8f30d651d16f9e7b6b0f2296cd71cec0", 
        "files": [
          {
            "content": "#!/bin/sh -ex\n\n# The MiniOS build is sensitive to some external variables\n# like EXTRA_DEPS being set, even to blank values.\n# See: https://github.com/mirage/mirage-xen-minios/issues/15\n# This wrapper script explicitly unexports them from the\n# environment before invoking Make\n\nunset EXTRA_DEPS\n\nmake debug=n CONFIG_VERBOSE_BOOT=n\n", 
            "name": "build-opam.sh"
          }
        ]
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "sh -ex ./build-opam.sh", 
          "make install LIBDIR=$cur__install/lib INCLUDEDIR=$cur__install/include", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "minios_xen_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "minios_xen_version": {
            "scope": "global", 
            "val": "0.9.0"
          }, 
          "minios_xen_enable": {
            "scope": "global", 
            "val": "enable"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml/substs", 
        "@opam/conf-perl": "*", 
        "opam-installer-bin": "esy-ocaml/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/minios-xen"
    }, 
