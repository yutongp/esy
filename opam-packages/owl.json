{
  "versions": {
    "0.1.0": {
      "version": "0.1.0", 
      "opam": {
        "url": "https://github.com/ryanrhymes/owl/archive/0.1.0.tar.gz", 
        "checksum": "67634d53317f90f2113401c17e3637c5", 
        "files": [
          {
            "content": "etc: [\n  \"setup.ml\"\n  \"setup.data\"\n  \"setup.log\"\n  \"_oasis_remove_.ml\"\n]\n", 
            "name": "owl.install"
          }
        ]
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "oasis setup", 
          "ocaml setup.ml -configure --prefix $cur__install", 
          "ocaml setup.ml -build", 
          "ocaml setup.ml -install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "owl_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "owl_version": {
            "scope": "global", 
            "val": "0.1.0"
          }, 
          "owl_enable": {
            "scope": "global", 
            "val": "enable"
          }
        }
      }, 
      "dependencies": {
        "@opam/ocamlfind": "*", 
        "@opam/oasis": ">= 0.4.0", 
        "@opam/plplot": "*", 
        "@opam/ctypes": "*", 
        "substs": "esy-ocaml/substs", 
        "@opam/dolog": "*", 
        "@opam/gsl": "*", 
        "@opam/conf-gsl": "*", 
        "opam-installer-bin": "esy-ocaml/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/owl"
    }, 
