{
  "versions": {
    "0.4.0": {
      "version": "0.4.0", 
      "opam": {
        "url": "https://github.com/hhugo/jsoolog/archive/0.4.tar.gz", 
        "checksum": "6c9b2af04be5a08cb6f86d26f0209f1c", 
        "files": [
          {
            "content": "lib: [\"default.css\"]", 
            "name": "joolog.install"
          }
        ]
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "oasis setup", 
          "ocaml setup.ml -configure --prefix $cur__install", 
          "ocaml setup.ml -build", 
          "ocaml setup.ml -install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "joolog_version": {
            "scope": "global", 
            "val": "0.4.0"
          }, 
          "joolog_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "joolog_enable": {
            "scope": "global", 
            "val": "enable"
          }
        }
      }, 
      "dependencies": {
        "@opam/ocamlfind": "*", 
        "@opam/oasis": "*", 
        "@opam/lwt": "*", 
        "@opam/react": "*", 
        "@opam/js_of_ocaml": "*", 
        "substs": "esy-ocaml/substs", 
        "@opam/camlp4": "*", 
        "opam-installer-bin": "esy-ocaml/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/joolog"
    }
  }
}