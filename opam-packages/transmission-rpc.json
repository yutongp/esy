        "exportedEnv": {
          "transmission_rpc_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "transmission_rpc_version": {
            "scope": "global", 
            "val": "1.0.0"
          }, 
          "transmission_rpc_enable": {
            "scope": "global", 
            "val": "enable"
          }
        }
      }, 
      "dependencies": {
        "@opam/ocamlfind": "*", 
        "@opam/ppx_deriving": "*", 
        "@opam/lwt": "*", 
        "@opam/ppx_deriving_yojson": ">= 2.0.0", 
        "@opam/yojson": "*", 
        "@opam/cohttp": ">= 0.13.0", 
        "substs": "esy-ocaml/substs", 
        "@opam/rresult": "*", 
        "opam-installer-bin": "esy-ocaml/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/transmission-rpc"
    }
  }
