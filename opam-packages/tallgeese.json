{
  "versions": {
    "0.1.0": {
      "version": "0.1.0", 
      "opam": {
        "url": "https://github.com/fxfactorial/tallgeese/archive/v0.1.tar.gz", 
        "checksum": "502cff039de5c07289078f2455cff3b6", 
        "files": [
          {
            "content": "open Printf\n\nlet () =\n  let dir = Sys.argv.(1) in\n  (try Sys.chdir dir\n   with _ -> eprintf \"Cannot change directory to %s\\n%!\" dir);\n  exit (Sys.command \"ocaml setup.ml -uninstall\")\n", 
            "name": "_oasis_remove_.ml"
          }, 
          {
            "content": "etc: [\n  \"setup.ml\"\n  \"setup.data\"\n  \"setup.log\"\n  \"_oasis_remove_.ml\"\n]\n", 
            "name": "tallgeese.install"
          }
        ]
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "oasis setup", 
          "ocaml setup.ml -configure --prefix $cur__install", 
          "ocaml setup.ml -build", 
          "ocaml setup.ml -install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "tallgeese_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "tallgeese_enable": {
            "scope": "global", 
            "val": "enable"
          }, 
          "tallgeese_version": {
            "scope": "global", 
            "val": "0.1.0"
          }
        }
      }, 
      "dependencies": {
        "@opam/ocamlfind": "*", 
        "@opam/oasis": ">= 0.4.0", 
        "@opam/libssh": "*", 
        "@opam/cmdliner": "*", 
        "substs": "esy-ocaml/substs", 
        "@opam/maxminddb": "*", 
        "opam-installer-bin": "esy-ocaml/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.02.3"
      }, 
      "name": "@opam/tallgeese"
    }
  }
}