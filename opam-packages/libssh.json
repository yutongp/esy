{
  "versions": {
    "0.1.0": {
      "version": "0.1.0", 
      "opam": {
        "url": "https://github.com/fxfactorial/ocaml-libssh/archive/v0.1.tar.gz", 
        "checksum": "5eea8bf42523058cd1a6c9bada7f5b3f", 
        "files": []
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "oasis setup", 
          "ocaml setup.ml -configure --prefix $cur__install", 
          "ocaml setup.ml -build", 
          "ocaml setup.ml -install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "libssh_enable": {
            "scope": "global", 
            "val": "enable"
          }, 
          "libssh_version": {
            "scope": "global", 
            "val": "0.1.0"
          }, 
          "libssh_installed": {
            "scope": "global", 
            "val": "true"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml/substs", 
        "@opam/ocamlfind": "*", 
        "@opam/oasis": ">= 0.4.0", 
        "opam-installer-bin": "esy-ocaml/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.02.3"
      }, 
      "name": "@opam/libssh"
    }
  }
}