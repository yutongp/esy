      "esy": {
        "buildsInSource": true, 
        "build": [
          "ocaml setup.ml -configure --prefix $cur__install --${base_unix_enable:-disable}-unix --${lwt_enable:-disable}-lwt --${async_enable:-disable}-async", 
          "ocaml setup.ml -build", 
          "ocaml setup.ml -install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "angstrom_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "angstrom_enable": {
            "scope": "global", 
