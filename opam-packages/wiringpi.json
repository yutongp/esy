{
  "versions": {
    "0.0.1": {
      "version": "0.0.1", 
      "opam": {
        "url": "https://github.com/Leonidas-from-XIV/ocaml-wiringpi/releases/download/0.0.1/ocaml-wiringpi-0.0.1.tar.gz", 
        "checksum": "46076ba8b12691c6a366189de0f86613", 
        "files": []
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "ocaml setup.ml -configure --prefix $cur__install", 
          "ocaml setup.ml -build", 
          "ocaml setup.ml -install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "wiringpi_version": {
            "scope": "global", 
            "val": "0.0.1"
          }, 
          "wiringpi_enable": {
            "scope": "global", 
            "val": "enable"
          }, 
          "wiringpi_installed": {
            "scope": "global", 
            "val": "true"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml/substs", 
        "@opam/ocamlfind": "*", 
        "opam-installer-bin": "esy-ocaml/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/wiringpi"
    }
  }
}