{
  "versions": {
    "0.1.0": {
      "version": "0.1.0", 
      "opam": {
        "url": "https://github.com/314eter/ocaml-xxhash/archive/v0.1.tar.gz", 
        "checksum": "c5142adfe682acb70a9b1705e920c0ef", 
        "files": []
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "ocaml setup.ml -configure --prefix $cur__install", 
          "ocaml setup.ml -build", 
          "ocaml setup.ml -install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "xxhash_version": {
            "scope": "global", 
            "val": "0.1.0"
          }, 
          "xxhash_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "xxhash_enable": {
            "scope": "global", 
            "val": "enable"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml/substs", 
        "@opam/ctypes": "*", 
        "@opam/oasis": "*", 
        "opam-installer-bin": "esy-ocaml/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/xxhash"
    }
  }
}