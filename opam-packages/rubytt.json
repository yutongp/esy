        "files": [
          {
            "content": "bin: [\"bin/main.native\" {\"rubytt\"}]", 
            "name": "rubytt.install"
          }
        ]
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "make native", 
          "make install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "rubytt_version": {
            "scope": "global", 
            "val": "0.1.0"
          }, 
          "rubytt_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "rubytt_enable": {
            "scope": "global", 
            "val": "enable"
          }
        }
      }, 
      "dependencies": {
        "@opam/ocamlfind": "*", 
        "@opam/alcotest": "*", 
        "@opam/yojson": "*", 
        "substs": "esy-ocaml/substs", 
        "@opam/stringext": "*", 
        "@opam/core": "*", 
        "@opam/ounit": "*", 
        "opam-installer-bin": "esy-ocaml/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": "> 4.01.0"
      }, 
      "name": "@opam/rubytt"
    }
  }
