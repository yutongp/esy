{
  "versions": {
    "0.0.1": {
      "version": "0.0.1", 
      "opam": {
        "url": "https://github.com/fxfactorial/ocaml-javascriptcore/archive/v0.0.1.tar.gz", 
        "checksum": "4dbccd3f09ceb97e970aa9b0a6a13a39", 
        "files": []
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "oasis setup", 
          "ocaml setup.ml -configure --prefix $cur__install", 
          "ocaml setup.ml -build", 
          "ocaml setup.ml -install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "javascriptcore_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "javascriptcore_version": {
            "scope": "global", 
            "val": "0.0.1"
          }, 
          "javascriptcore_enable": {
            "scope": "global", 
            "val": "enable"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml/substs", 
        "@opam/ocamlfind": "*", 
        "@opam/oasis": ">= 0.4.0", 
        "opam-installer-bin": "esy-ocaml/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.03.0"
      }, 
      "name": "@opam/javascriptcore"
    }
  }
}