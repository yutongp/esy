        "checksum": "c2d5f085a0aa0f6b5cf7cac2568560fb", 
        "files": []
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "ocaml setup.ml -configure --prefix $cur__install --${lwt_enable:-disable}-lwt", 
          "ocaml setup.ml -build", 
          "ocaml setup.ml -install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "sessions_enable": {
            "scope": "global", 
            "val": "enable"
          }, 
          "sessions_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "sessions_version": {
            "scope": "global", 
            "val": "0.1.0"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml/substs", 
        "@opam/ocamlfind": "*", 
        "@opam/lwt": "*", 
        "opam-installer-bin": "esy-ocaml/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.02.3"
      }, 
      "name": "@opam/sessions"
    }
  }
}