{
  "versions": {
    "0.8.1": {
      "version": "0.8.1", 
      "opam": {
        "url": "http://complogic.cs.mcgill.ca/beluga/beluga-0.8.1.tar.gz", 
        "checksum": "f5e940e1f96d9fa6a5ce5ef954be98e7", 
        "files": [
          {
            "content": "bin: [\"_build/src/beluga/main.native\" {\"beluga\"}]\nshare: [\"tools/beluga-mode.el\"]\n", 
            "name": "beluga.install"
          }
        ]
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "ocamlbuild -r -use-ocamlfind -j 4 src/beluga/main.native", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "beluga_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "beluga_version": {
            "scope": "global", 
            "val": "0.8.1"
          }, 
          "beluga_enable": {
            "scope": "global", 
            "val": "enable"
          }
        }
      }, 
      "dependencies": {
        "@opam/ocamlfind": "*", 
        "@opam/extlib": "*", 
        "substs": "esy-ocaml/substs", 
        "@opam/extlib-compat": "*", 
        "@opam/ulex": "*", 
        "opam-installer-bin": "esy-ocaml/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/beluga"
    }, 
