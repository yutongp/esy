        "files": [
          {
            "content": "bin: [\n  \"imagen\"\n  \"_build/hevea.native\" {\"hevea\"}\n  \"_build/hacha.native\" {\"hacha\"}\n  \"_build/esponja.native\" {\"esponja\"}\n  \"_build/bibhva.native\" {\"bibhva\"}\n]\n", 
            "name": "hevea.install"
          }
        ]
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "make PREFIX=$cur__install", 
          "make install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "hevea_enable": {
            "scope": "global", 
            "val": "enable"
          }, 
          "hevea_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "hevea_version": {
            "scope": "global", 
            "val": "2.01.0"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml/substs", 
        "@opam/ocamlfind": "*", 
        "opam-installer-bin": "esy-ocaml/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/hevea"
    }, 
