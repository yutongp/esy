{
  "versions": {
    "0.1.0": {
      "version": "0.1.0", 
      "opam": {
        "url": "https://github.com/masateruk/micro-caml/archive/v0.1.tar.gz", 
        "checksum": "d908ba04acf795c410616e83bfd663b6", 
        "files": [
          {
            "content": "bin: [\n  \"ucaml\"\n]\n", 
            "name": "ucaml.install"
          }
        ]
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "make", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "ucaml_enable": {
            "scope": "global", 
            "val": "enable"
          }, 
          "ucaml_version": {
            "scope": "global", 
            "val": "0.1.0"
          }, 
          "ucaml_installed": {
            "scope": "global", 
            "val": "true"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml/substs", 
        "opam-installer-bin": "esy-ocaml/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/ucaml"
    }
  }
