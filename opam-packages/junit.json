{
  "versions": {
    "0.1.0": {
      "version": "0.1.0", 
      "opam": {
        "url": "https://github.com/Khady/ocaml-junit/archive/0.1.zip", 
        "checksum": "b25790f915ed08bd87796b3d722e0fd0", 
        "files": []
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "ocaml setup.ml -configure --prefix $cur__install", 
          "ocaml setup.ml -build", 
          "ocaml setup.ml -install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "junit_version": {
            "scope": "global", 
            "val": "0.1.0"
          }, 
          "junit_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "junit_enable": {
            "scope": "global", 
            "val": "enable"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml/substs", 
        "@opam/ocamlfind": "*", 
        "@opam/ptime": "*", 
        "opam-installer-bin": "esy-ocaml/opam-installer-bin", 
        "@opam/tyxml": ">= 4.0.0"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.02.1"
      }, 
      "name": "@opam/junit"
    }
  }
}