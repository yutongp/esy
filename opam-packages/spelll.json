        "checksum": "e4574039054e3850357ea434dfcac039", 
        "files": []
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "./configure --docdir $cur__doc/spelll/", 
          "make all", 
          "make install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "spelll_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "spelll_version": {
            "scope": "global", 
            "val": "0.1.0"
          }, 
          "spelll_enable": {
            "scope": "global", 
            "val": "enable"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml/substs", 
        "@opam/ocamlfind": "*", 
        "opam-installer-bin": "esy-ocaml/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.00.0"
      }, 
      "name": "@opam/spelll"
    }
  }
}