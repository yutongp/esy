        "checksum": "94fa6040cf7d4c4304cb06fa5902a1aa", 
        "files": []
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "oasis setup", 
          "ocaml setup.ml -configure --prefix $cur__install", 
          "ocaml setup.ml -build", 
          "cp opam/files/oloop.install ./", 
          "ocaml setup.ml -install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "oloop_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "oloop_version": {
            "scope": "global", 
            "val": "0.1.2"
          }, 
          "oloop_enable": {
            "scope": "global", 
            "val": "enable"
          }
        }
      }, 
      "dependencies": {
        "@opam/ocamlfind": ">= 1.5.0", 
        "@opam/oasis": ">= 0.4.0", 
        "@opam/async": "*", 
        "@opam/cppo": "*", 
        "substs": "esy-ocaml/substs", 
        "@opam/core_kernel": "*", 
        "@opam/sexplib": "< 113.01.00", 
        "opam-installer-bin": "esy-ocaml/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.01.0 < 4.03.0"
      }, 
      "name": "@opam/oloop"
    }
  }
