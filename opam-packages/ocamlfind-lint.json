{
  "versions": {
    "0.1.0": {
      "version": "0.1.0", 
      "opam": {
        "url": "http://github.com/zoggy/findlib-lint/archive/v0.1.0.tar.gz", 
        "checksum": "74b3f70eac0cabcd51b317fc73dbfc69", 
        "files": [
          {
            "content": "bin: [\n  \"?ocamlfind-lint\"\n  \"?ocamlfind-lint.byte\"\n]\n", 
            "name": "ocamlfind-lint.install"
          }
        ]
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "make all", 
          "make install-lib", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "ocamlfind_lint_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "ocamlfind_lint_version": {
            "scope": "global", 
            "val": "0.1.0"
          }, 
          "ocamlfind_lint_enable": {
            "scope": "global", 
            "val": "enable"
          }
        }
      }, 
      "dependencies": {
        "substs": "esy-ocaml/substs", 
        "@opam/ocamlfind": "*", 
        "opam-installer-bin": "esy-ocaml/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/ocamlfind-lint"
    }
  }
}