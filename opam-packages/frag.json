{
  "versions": {
    "0.1.0": {
      "version": "0.1.0", 
      "opam": {
        "url": "http://pw374.github.io/distrib/frag/frag-0.1.0.tar.gz", 
        "checksum": "934aeba80b96ef75d5bd8b366e7b1a18", 
        "files": [
          {
            "content": "bin: [\n  \"?_build/src/fragment_main.byte\" {\"frag\"}\n  \"?_build/src/fragment_main.native\" {\"frag\"}\n]\n", 
            "name": "frag.install"
          }
        ]
      }, 
      "esy": {
        "buildsInSource": true, 
        "build": [
          "ocaml setup.ml -configure --prefix $cur__install", 
          "ocaml setup.ml -build", 
          "ocaml setup.ml -install", 
          "(opam-installer --prefix=$cur__install || true)"
        ], 
        "exportedEnv": {
          "frag_installed": {
            "scope": "global", 
            "val": "true"
          }, 
          "frag_version": {
            "scope": "global", 
            "val": "0.1.0"
          }, 
          "frag_enable": {
            "scope": "global", 
            "val": "enable"
          }
        }
      }, 
      "dependencies": {
        "@opam/ocamlfind": "*", 
        "@opam/mpp": "*", 
        "substs": "esy-ocaml/substs", 
        "@opam/re": "*", 
        "@opam/core": ">= 109.42.00", 
        "opam-installer-bin": "esy-ocaml/opam-installer-bin"
      }, 
      "scripts": {}, 
      "peerDependencies": {
        "ocaml": ">= 4.2.3"
      }, 
      "name": "@opam/frag"
    }
  }
}